<div class="posts col-md-6">
  <% @posts.each do |post| %>
  <div class="panel panel-default">
    <div class="panel-heading"><%= link_to post.title , "/posts/#{post.id}" %> by <%= link_to post.user.usermane, "/users/#{post.user.id}" %> in <%= post.category.name %></div>
    <div class="panel-body">
      <%= image_tag post.picture.url(:medium), class: "thumbnail", style: "width: 100px" %>
      <p><%= post.description %></p>
      <p><%= link_to "Demander" , "/posts/#{post.id}", class: "btn btn-default"%></p>
    </div>
  </div>
  <% end %>
</div>

<div class="maps col-md-6">
  <div style='width: 100%;'>
    <div id="map" class="thumbnail" style='width: 100%; height: 500px;'></div>
  </div>
</div>
